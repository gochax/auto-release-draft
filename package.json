{
  "name": "auto-release-draft",
  "version": "0.0.0",
  "private": true,
  "description": "Drafts a GitHub rlease with the changes introduced by a newly created version tag.",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint src/**/*.ts",
    "pack": "ncc build",
    "test": "jest",
    "all": "npm run build && npm run format && npm run lint && npm run pack && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gochax/auto-release-draft.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "author": "Chris Bo",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "1.3.0",
    "@actions/github": "4.0.0"
  },
  "devDependencies": {
    "@types/jest": "24.9.1",
    "@types/node": "14.17.1",
    "@types/semver": "7.3.6",
    "@typescript-eslint/parser": "2.10.0",
    "@zeit/ncc": "0.22.3",
    "eslint": "5.16.0",
    "eslint-plugin-github": "2.0.0",
    "eslint-plugin-jest": "22.21.0",
    "jest": "^24.9.0",
    "jest-circus": "^24.9.0",
    "js-yaml": "3.14.1",
    "jest": "27.0.0",
    "jest-circus": "27.0.0",
    "js-yaml": "3.14.1",
    "prettier": "2.3.0",
    "ts-jest": "24.2.0",
    "typescript": "3.9.9"
  }
}
